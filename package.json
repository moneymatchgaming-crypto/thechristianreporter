{
  "name": "christian-reporter",
  "version": "1.0.0",
  "description": "Real-time Christian news aggregation with faith-based content",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server.cjs",
    "server:dev": "nodemon server.cjs",
    "start": "node server.cjs",
    "build:prod": "npm run build && echo 'Frontend built successfully'",
    "deploy:check": "npm run build && npm run lint",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "keywords": [
    "christian",
    "news",
    "faith",
    "religion",
    "church",
    "bible",
    "ministry",
    "evangelical"
  ],
  "author": "The Christian Reporter",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/christian-reporter"
  },
  "homepage": "https://thechristianreporter.com"
}
